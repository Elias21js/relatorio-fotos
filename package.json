    "firebase": "^11.9.0",
    "flatpickr": "^4.6.13",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "swiper": "^11.2.8",
    "uuid": "^11.1.0",
    "uuidv4": "^6.2.13"
